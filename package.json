{
  "name": "kichikuou-web",
  "version": "1.0.0",
  "description": "",
  "repository": "github:kichikuou/web",
  "scripts": {
    "type": "cd shell && tsc",
    "build-shell": "esbuild shell/shell.ts --bundle --target=es2017 --format=esm --outfile=docs/shell.js --charset=utf8 --sourcemap --minify",
    "watch-shell": "npm run build-shell -- --watch",
    "build-css": "esbuild --bundle css/style.css --outfile=docs/style.css --supported:nesting=false"
  },
  "dependencies": {
    "@types/emscripten": "1.39.5",
    "@types/gtag.js": "^0.0.12",
    "@types/jszip": "0.0.33"
  },
  "devDependencies": {
    "esbuild": "^0.17.19",
    "typescript": "^4.9.5"
  }
}
